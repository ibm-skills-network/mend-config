{
  "settingsInheritedFrom": "ibm-mend-config/mend-config@main",
  "overrideConfigAllowList": [],
  "scanSettings": {
    "configMode": "EXTERNAL",
    "configExternalURL": "https://raw.githubusercontent.com/ibm-skills-network/mend-config/main/whitesource.config",
    "exploitability": true
  },
  "checkRunSettings": {
    "strictMode": "warning",
    "strictModeInfo": true
  },
  "issueSettings": {
    "issueRepoName": "security-compliance-automation"
  },
  "remediateSettings": {
    "enableRenovate": true,
    "extends": [
      "config:recommended",
      ":disableMajorUpdates",
      "docker:disable"
    ],
    "transitiveRemediation": true
  }
}
